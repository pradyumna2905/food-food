<%= render 'shared/page_title', title: "LOG IN"%>


<div class="row">
  <!--<div class="well col-md-6 col-md-offset-3">-->
  <div class="jumbotron well login-form">
    <%= form_tag '/login' do %>

    <!--<%= label_tag :email %>-->
    <%= text_field_tag :email, nil, class: "form-input form-input-top", placeholder: "Email" %><br />

    <!--<%= label_tag :password %>-->
    <%= password_field_tag :password, nil, class: "form-input form-input-bottom", placeholder: "Password" %>
    <%= submit_tag 'Log In', class: 'btn btn-success login-btn' %>

    <% end%>
  </div>
</div>
